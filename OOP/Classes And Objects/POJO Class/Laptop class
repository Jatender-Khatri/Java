public class Main
{
   private  String laptopName, generation;
   private  int price;

    public void setLaptopName(String laptopName) {
        this.laptopName = laptopName;
    }

    public void setPrice(int price) {
        this.price = price;
    }

    public void setGeneration(String generation) {
        this.generation = generation;
    }

    public int getPrice() {
        return price;
    }

    public String getGeneration() {
        return generation;
    }

    public String getLaptopName() {
        return laptopName;
    }
    public static void main(String args[])
    {
        Main m = new Main();
        m.setPrice(40000);
        m.setGeneration("4th Generation");
        m.setLaptopName("HP");
        System.out.println(m.getPrice() + "  " + m.getGeneration() + "  " + m.getLaptopName());
    }
}
