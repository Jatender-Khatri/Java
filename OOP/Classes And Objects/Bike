class Bike
{
	String BikeName;
	String Model;
	int price;
	
	public void Bike125()
	{
		System.out.println("125 Bike");
	}
	public void HondaBike(){
		System.out.println("Honda Bike");
	}
	public static void main(String args[])
	{
		Bike b = new Bike();
		b.BikeName = "125";
		b.Model = "2019";
		b.price = 78000;
		System.out.println(b.BikeName);
		System.out.println(b.Model);
		System.out.println(b.price);
		b.Bike125();
		b.HondaBike();
	}
}
